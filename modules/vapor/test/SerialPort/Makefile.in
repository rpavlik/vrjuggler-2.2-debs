# -----------------------------------------------------------------------------
# Makefile.in for vapor/vpr/test/SerialPort
#
# Generated for use on @PLATFORM@
# -----------------------------------------------------------------------------

# Generated by mkmakefile.pl using the following options:
#    --apps reader=reader.cpp --apps writer=writer.cpp --apps
#    first=first.cpp --apps second=second.cpp

default: all

# Basic options.
srcdir		= @srcdir@
CFLAGS		= @APP_CFLAGS@ $(EXTRA_CFLAGS) $(INCLUDES) $(DEFS)
CXXFLAGS	= @APP_CXXFLAGS@ $(EXTRA_CFLAGS) $(INCLUDES) $(DEFS)
DEFS		= @APP_DEFS@
EXTRA_CFLAGS	= @APP_EXTRA_CFLAGS@ $(DEBUG_CFLAGS)
DEBUG_CFLAGS	= @APP_DEBUG_CFLAGS@
OPTIM_CFLAGS	= @APP_OPTIM_CFLAGS@
INCLUDES	= @APP_INCLUDES@ @BOOST_INCLUDES@ -I$(srcdir)
EXTRA_LFLAGS	= @APP_EXTRA_LFLAGS@ $(DEBUG_LFLAGS)
DEBUG_LFLAGS	= @APP_DEBUG_LFLAGS@
OPTIM_LFLAGS	= @APP_OPTIM_LFLAGS@
LINK_FLAGS	= @APP_LINK_FLAGS@ $(EXTRA_LFLAGS)
LINKALL_ON	= @APP_LINKALL_ON@
LINKALL_OFF	= @APP_LINKALL_OFF@

# Libraries needed for linking.
BASIC_LIBS	= @APP_BASIC_LIBS_BEGIN@ @APP_BASIC_LIBS@  @APP_BASIC_EXT_LIBS@ @APP_BASIC_LIBS_END@
EXTRA_LIBS	= @APP_EXTRA_LIBS_BEGIN@ @APP_EXTRA_LIBS@  @APP_EXTRA_LIBS_END@ 

# Commands to execute.
C_COMPILE	= @APP_CC@ $(CFLAGS)
CXX_COMPILE	= @APP_CXX@ $(CXXFLAGS)
LINK		= @APP_LINK@ $(LINK_FLAGS)

VPATH	= @srcdir@

first_OBJS	= first.@OBJEXT@

reader_OBJS	= reader.@OBJEXT@

second_OBJS	= second.@OBJEXT@

writer_OBJS	= writer.@OBJEXT@

# -----------------------------------------------------------------------------
# Application build targets.
# -----------------------------------------------------------------------------
all: first@EXEEXT@ reader@EXEEXT@ second@EXEEXT@ writer@EXEEXT@

first@EXEEXT@: $(first_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(first_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

reader@EXEEXT@: $(reader_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(reader_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

second@EXEEXT@: $(second_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(second_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

writer@EXEEXT@: $(writer_OBJS)
	$(LINK) @EXE_NAME_FLAG@ $(writer_OBJS) $(BASIC_LIBS) $(EXTRA_LIBS)

# Suffix rules for building object files.
.SUFFIXES: .cpp .@OBJEXT@

.cpp.@OBJEXT@:
	$(CXX_COMPILE) @OBJ_NAME_FLAG@ @OBJ_BUILD_FLAG@ $<

# -----------------------------------------------------------------------------
# Clean-up targets.
# -----------------------------------------------------------------------------
clean:
	rm -f Makedepend *.@OBJEXT@ first.ilk reader.ilk second.ilk writer.ilk  so_locations *.?db core*
	rm -rf ii_files

clobber:
	@$(MAKE) clean
	rm -f first@EXEEXT@ reader@EXEEXT@ second@EXEEXT@ writer@EXEEXT@ 
